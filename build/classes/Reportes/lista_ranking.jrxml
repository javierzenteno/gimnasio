	<parameter name="codtor" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[select torneo.codtor,nomtor,fector,destor,esttor,codmod,codgen,jugador.codjug,postor,concat(nomper,' ',priapper,' ',segapper)jugador from torneo
